{
  "name": "cron-api",
  "version": "0.1.0",
  "description": "Cron API",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node dist",
    "build": "tsc && tsc-alias",
    "build-check": "tsc --noEmit",
    "dev": "tsc -w & tsc-alias -w & node --watch dist",
    "lint": "eslint . --ext .ts,.js --ignore-path .gitignore --max-warnings=0",
    "format": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/plataforma-5/cron-api.git"
  },
  "author": "Plataforma5",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/plataforma-5/cron-api/issues"
  },
  "homepage": "https://gitlab.com/plataforma-5/cron-api#readme",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.342.0",
    "axios": "^1.4.0",
    "cron": "^2.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/morgan-json": "^1.1.0",
    "@types/node": "^20.2.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.9",
    "morgan-json": "^1.1.0",
    "prettier": "^2.8.8",
